AppBundle\Entity\PriceLists:
    type: entity
    repositoryClass: AppBundle\Repository\PriceListsRepository
    table: PriceLists
    id:
        id:
            type: integer
            id: true
            generator:
                strategy: AUTO
            options:
                unsigned: true
    fields:        
        name:
            type: string
            nullable: false
            length: 4
            fixed: false
        description:
            type: string
            nullable: true
            length: 20
            fixed: false
            default: ''            
        start:
            type: datetime
        end:
            type: datetime   
            nullable: true
    oneToMany:
        prices:
            targetEntity: Prices
            mappedBy: priceList
            cascade: [ "persist" ]
    manyToMany:
        clients:
            targetEntity: Clients
            mappedBy: priceLists
        clientsGroups:
            targetEntity: ClientsGroups
            mappedBy: priceLists
        notes:
            targetEntity: Notes
            joinTable:
                name: PriceLists_Notes
                joinColumns:
                    priceLists_id:
                        referencedColumnName: id
                inverseJoinColumns:
                    notes_id:
                        referencedColumnName: id 
            cascade: [ "persist" ]
    lifecycleCallbacks: 
        prePersist: [ prePersistPriceLists ]